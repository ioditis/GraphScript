kvadrataMala = 50
kvadrataMala2 = kvadrataMala/2

pA = Point( 10, 10 )

pB = pA.Right( kvadrataMala )
pC = pB.Down( kvadrataMala )
pD = pC.Left( kvadrataMala )
pE = pA.Right( kvadrataMala2 )

kvAB= Line(pA, pB)
kvBC= Line(pB, pC)
kvCD= Line(pC, pD)
kvDA= Line(pD, pA)

trijstEC = Line(pE, pC)
trijstCD = Line(pC, pD)
trijstDE = Line(pD, pE)


